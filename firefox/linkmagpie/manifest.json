{
    "manifest_version": 2,
    "name": "linkmagpie",
    "version": "1.0",

    "description": "Adds the fuzzy search link selection and following",

    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["linkmagpie.js"]
        }
    ]
}